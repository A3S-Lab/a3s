[package]
name = "a3s-privacy"
version = "0.1.0"
edition = "2021"
authors = ["A3S Lab"]
license = "MIT"
description = "Privacy classification and data protection for A3S"
repository = "https://github.com/A3S-Lab/a3s"
keywords = ["a3s", "privacy", "classification", "redaction"]
categories = ["text-processing"]

[dependencies]
regex = "1"
serde = { version = "1", features = ["derive"] }
serde_json = "1"
thiserror = "1"
